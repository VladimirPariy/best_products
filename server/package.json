  "main": "dist/index.js",
  "type": "commonjs",
  "module": "./src",
  "scripts": {
    "start": "tsc && node dist/index.js",
    "dev": "nodemon --watch src src/index.ts",
    "up": "knex migrate:up --knexfile src/database/knexfile.ts",
